                      <a href="/telefone/conexao" class="btn btn-block btn-primary">
                        <i ></i> Gerar QRcode
                      </a>      
                    </div>  
                  </div>
                  <div class="col-sm-4">
                   
                  </div>
                  <div class="col-sm-4">
                   
                  </div>
                </div>
                <div class="row mt-4">
                  <div class="col-sm-4">
                    <div class="position-relative">
                      <img style="height: 250px;" src="<%= qrcode %>" alt="Photo 1" class="img-fluid">

                      <div class="ribbon-wrapper ribbon-lg">                        
